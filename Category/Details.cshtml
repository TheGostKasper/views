@model AMS.Models.Category

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}<header class="m-header">
    <h2 class="m-title">Details</h2>
</header>
<h2 class="title"><a class="btn btn-secondary btn--icon-text waves-effect" href="~/Category/"><span><i class="zmdi zmdi-arrow-back"></i> Back</span></a></h2>


<div class="card-block">
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NameEN)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NameEN)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NameAR)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NameAR)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CountryId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CountryId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ImageUrl)
        </dt>
        <dd class="col-sm-10">
            <img id="image_view" src="@Model.ImageUrl" alt="cat_img" />
        </dd>
    </dl>
</div>
<p>
    <a class="btn btn-secondary btn--icon-text" href="~/Category/Edit/@Model.Id"><span><i class="zmdi zmdi-edit zmdi-hc-fw"></i>Edit</span></a>
</p>
